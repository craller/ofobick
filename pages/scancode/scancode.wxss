/* pages/scancode/scancode.wxss */
.container{
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.password-title,.tips{
   flex: 1;
   padding: 40rpx 0;
}
.password-content{
  font-size: 200rpx;
  flex: 8;
  font-weight: 900;
}
.tips{
  font-size: 32rpx;
}
.tips .tips-action{
   margin-top: 20rpx;
}
.tips .tips-href{
  color: #b9dd08;
}
